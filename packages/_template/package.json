{
  "name": "@dk/template",
  "private": true,
  "version": "1.0.0",
  "description": "Serves as a template for workspace packages",
  "scripts": {
    "format": "prettier --write --cache .",
    "lint": "eslint --fix --cache --ext ts src",
    "typecheck": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": "@dk/eslint-config"
  },
  "devDependencies": {
    "@dk/eslint-config": "workspace:*",
    "@dk/tsconfig": "workspace:*"
  }
}
